 293  ******************UNIFAC******************************************************
 294  *Process equations
 295  eq_z..                                  z =e= yield('ibuprofen');
 296  eq_yield(jj)..                          yield(jj) =e= 1 - (nm(jj,'cooled') / nm(jj,'heated'));
 297  eq_soluse..                             xi_s * (mass('ibuprofen','heated') - mass('ibuprofen','cooled')) =e= mass('s1','cooled') + mass('s2','cooled');
 298  eq_volsoluse..                          v_s * (mass('ibuprofen','heated') - mass('ibuprofen','cooled')) =e= sum(ii,vol(ii));
 299  eq_x(st)..                              sum(i, x(i,st)) =e= 1;
 300  eq_melt(ii,s)..                         y(ii,s)*prop(s,'p2') =l= T('cooled') - Tos;
 301  eq_boil(ii,s)..                         prop(s,'p1') =g= (y(ii,s) * (T('heated') + Tos));
 302  eq_T1(s)..                              T('heated') =l= prop(s,'p1')-Tos + 600*(1 - sum(ii, y(ii,s)));
 303  eq_T2(s)..                              T('cooled') =g= prop(s,'p2')+Tos - 600*(1 - sum(ii, y(ii,s)));
 304  eq_T(s)..                               T('heated') =g= T('cooled');
 305   
 306  eq_s1..                                 nm('s1','heated') =e= nm('s1','cooled');
 307  eq_s2..                                 nm('s2','heated') =l= nm('s2','cooled');
 308  eq_API(jj)..                            nm(jj,'heated') =g= nm(jj,'cooled');
 309  eq_nmt(st)..                            nmt(st) =e= sum(i, nm(i,st));
 310  eq_nm(i,st)..                           x(i,st) =e= nm(i,st)/nmt(st);
 311   
 312  eq_Mw(ii)..                             Mw(ii) =e= sum(s, prop(s, 'p3') * y(ii,s));
 313  eq_Mwib(jj)..                           Mw(jj) =e= Mwib(jj);
 314  eq_mass(i,st)..                         mass(i,st) =e= nm(i,st) * Mw(i);
 315  eq_den(ii)..                            den(ii) =e= sum(s, prop(s,'p4')*y(ii,s));
 316  eq_vol(ii)..                            vol(ii) * den(ii) =e= mass(ii,'cooled');
 317  eq_n1(ii,k)..                           n(ii,k) =e= sum(s, v(s,k)*y(ii,s));
 318  eq_n2(jj,k)..                           n(jj,k) =e= vib(jj,k);
 319   
 320  *****Solubility calculations using UNIFAC
 321  eq_qc1(ii)..                            qc(ii) =e= sum(s,qs(s) * y(ii,s));
 322  eq_rc1(ii)..                            rc(ii) =e= sum(s,rs(s) * y(ii,s));
 323  eq_qc2(jj)..                            qc(jj) =e= qib(jj);
 324  eq_rc2(jj)..                            rc(jj) =e= rib(jj);
 325   
 326  *Combinatorial part of the activity coefficient
 327  eq_J(jj,st)..                           J(jj,st) * sum(i,x(i,st)*rc(i)) =e= rib(jj);
 328  eq_L(jj,st)..                           L(jj,st) * sum(i,x(i,st)*qc(i)) =e= qib(jj);
 329  eq_lngc(jj,st)..                        lngc(jj,st) =e= 1 - J(jj,st) + log(J(jj,st)) - 5*qib(jj)*(1 - J(jj,st)/L(jj,st) + log(J(jj,st)) - log(L(jj,st)));
 330   
 331  *Residual part of the activity coefficient
 332  eq_ps(k,m,st)..                         ps(k,m,st) =e= exp(-a(k,m)/T(st));
 333  eq_bib(jj,k,st)..                       bib(jj,k,st) =e= sum(m,eib(jj,m)*ps(m,k,st));
 334  eq_th(k,st)..                           th(k,st)*sum(i,x(i,st)*qc(i)) =e= sum(i,x(i,st)*GS(k,'Q')*n(i,k));
 335  eq_w(k,st)..                            w(k,st) =e= sum(m,th(m,st)*ps(m,k,st));
 336  eq_lngr(jj,st)..                        lngr(jj,st) =e= qib(jj)*(1-sum(k,th(k,st)*bib(jj,k,st)/w(k,st)-eib(jj,k)*(log(bib(jj,k,st)) - log(w(k,st)))));
 337   
 338  *Overall activity coefficient
 339  eq_lng(jj,st)..                         lng(jj,st) =e= lngc(jj,st) + lngr(jj,st);
 340   
 341  *Solubility oonstraint
 342  eq_solub1(st)..                         log(x('ibuprofen',st)) + lng('ibuprofen',st) =e= Hm('ibuprofen')/Rg * (1/Tm('ibuprofen') - 1/T(st));
 343   
 344  ***********************************Miscibility between s1 & s2**********
 345  eq_x12a(st)..x12('s1',st)*(x('s1',st)+x('s2',st))=e=x('s1',st);
 346  eq_x12b(st)..x12('s1',st)+x12('s2',st)=e=1;
 347   
 348  eq_sumr12(st)..sumr12(st) =e= x12('s1',st)*rc('s1')+(1-x12('s1',st))*rc('s2');
 349   
 350  eq_sumq12(st)..sumq12(st)=e=x12('s1',st)*qc('s1')+(1-x12('s1',st))*qc('s2');
 351   
 352  *differ of combinatorial activity coefficient, i.e. dlngc/dx(s1)
 353  eq_dlngc12(st)..d_lngc12('s1',st)*power(sumr12(st),2)*sumq12(st)=e=5*qc('s1')*(qc('s2')-qc('s1'))*power(sumr12(st),2)
 354                                          +(rc('s1')-rc('s2'))*(5*qc('s1')-1)*sumr12(st)*sumq12(st)
 355                                          +5*rc('s1')*(qc('s1')-qc('s2'))*sumr12(st)*sumq12(st)
 356                                          +((rc('s1')-rc('s2'))*(rc('s1')-5*rc('s1')*(sumq12(st))))*sumq12(st);
 357   
 358  eq_th12(k,st)..th12(k,st)*sumq12(st)=e=x12('s1',st)*GS(k,'Q')*n('s1',k)+(1-x12('s1',st))*GS(k,'Q')*n('s2',k);
 359  eq_w12(k,st)..w12(k,st)=e=sum(m,th12(m,st)*ps(m,k,st));
 360  eq_dth12(k,st)..d_th12(k,st)*power(sumq12(st),2)=e=((GS(k,'Q')*n('s1',k)- GS(k,'Q')*n('s2',k))*sumq12(st))-(x12('s1',st)*GS(k,'Q')*n('s1',k)+ x12('s2',st)*GS(k,'Q')*n('s2',k))*(qc('s1')-qc('s2'));
 361  eq_dw12(k,st)..d_w12(k,st)=e=sum(m,ps(m,k,st)*d_th12(m,st));
 362  eq_a12(k,st)..a12(k,st)*power(w12(k,st),2)=e=d_th12(k,st)*w12(k,st)-th12(k,st)*d_w12(k,st);
 363  eq_b12(k,st)..b12('s1',k,st)=e=sum(m,GS(m,'Q')*n('s1',m)*ps(m,k,st));
 364  eq_c12(k,st)..c12(k,st)*w12(k,st)=e=d_w12(k,st);
 365   
 366  *differ of residual activity coefficient, i.e. dlngr/dx(s1)
 367  *simplier form, not the general form of unifac
 368  eq_dlngr12(st)..d_lngr12('s1',st)=e=-sum(k,a12(k,st)*b12('s1',k,st)+GS(k,'Q')*n('s1',k)*c12(k,st));
 369   
 370  *differ of activity coefficient,i.e. dlng/dx(s1)
 371  eq_dlng12(st)..d_lng12('s1',st)=e=d_lngc12('s1',st)+d_lngr12('s1',st);
 372   
 373  eq_misc12(st)..d_lng12('s1',st)*x12('s1',st)+1=g=0;
