 424  eq_inp1(st)..                                                   inp('1',st) =e= input_gain('1')*(qc('s1') - input_offset('1'));
 425  eq_inp2(st)..                                                   inp('2',st) =e= input_gain('2')*(qc('s2') - input_offset('2'));
 426  eq_inp3(st)..                                                   inp('3',st) =e= input_gain('3')*(rc('s1') - input_offset('3'));
 427  eq_inp4(st)..                                                   inp('4',st) =e= input_gain('4')*(rc('s2') - input_offset('4'));
 428  eq_inp5(st)..                                                   inp('5',st) =e= input_gain('5')*(x('s1',st) - input_offset('5'));
 429  eq_inp6(st)..                                                   inp('6',st) =e= input_gain('6')*(x('s2',st) - input_offset('6'));
 430  eq_inp7(st)..                                                   inp('7',st) =e= input_gain('7')*(T(st) - input_offset('7'));
 431   
 432  eq_inter1v(hl1,st)..                                            inter1v(hl1,st) =e= sum(f, wt1(f,hl1)*inp(f,st)) + bias1(hl1);
 433  eq_inter1z(hl1,st)..                                            inter1z(hl1,st) =e= 1 - (2/(exp(2*inter1v(hl1,st)) + 1));
 434  eq_inter2v(hl2,st)..                                            inter2v(hl2,st) =e= sum(hl1, wt2(hl1,hl2)*inter1z(hl1,st)) + bias2(hl2);
 435  eq_inter2z(hl2,st)..                                            inter2z(hl2,st) =e= 1 - (2/(exp(2*inter2v(hl2,st)) + 1));
 436  eq_inter3v(hl3,st)..                                            inter3v(hl3,st) =e= sum(hl2, wt3(hl2,hl3)*inter2z(hl2,st)) + bias3(hl3);
 437  eq_inter3z(hl3,st)..                                            inter3z(hl3,st) =e= 1 - (2/(exp(2*inter3v(hl3,st)) + 1));
 438  eq_inter4v(st)..                                                inter4v(st) =e= sum(hl3, wt4(hl3)*inter3z(hl3,st)) + bias4;
 439  eq_inter4z(st)..                                                inter4z(st) =e= 1/(1 + exp(-inter4v(st)));
 440  eq_dlng12(st)..                                                 misc12(st) =e= inter4z(st);
