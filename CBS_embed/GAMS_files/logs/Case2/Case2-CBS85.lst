 360  ******************UNIFAC******************************************************
 361  *Process equations
 362  eq_z..                                  z =e= yield('ibuprofen');
 363  eq_yield(jj)..                          yield(jj) =e= 1 - (nm(jj,'cooled') / nm(jj,'heated'));
 364  eq_soluse..                             xi_s * (mass('ibuprofen','heated') - mass('ibuprofen','cooled')) =e= mass('s1','cooled') + mass('s2','cooled');
 365  eq_volsoluse..                          v_s * (mass('ibuprofen','heated') - mass('ibuprofen','cooled')) =e= sum(ii,vol(ii));
 366  eq_x(st)..                              sum(i, x(i,st)) =e= 1;
 367  eq_melt(ii,s)..                         y(ii,s)*prop(s,'p2') =l= T('cooled') - Tos;
 368  eq_boil(ii,s)..                         prop(s,'p1') =g= (y(ii,s) * (T('heated') + Tos));
 369  eq_T1(s)..                              T('heated') =l= prop(s,'p1')-Tos + 600*(1 - sum(ii, y(ii,s)));
 370  eq_T2(s)..                              T('cooled') =g= prop(s,'p2')+Tos - 600*(1 - sum(ii, y(ii,s)));
 371  eq_T(s)..                               T('heated') =g= T('cooled');
 372   
 373  eq_s1..                                 nm('s1','heated') =e= nm('s1','cooled');
 374  eq_s2..                                 nm('s2','heated') =l= nm('s2','cooled');
 375  eq_API(jj)..                            nm(jj,'heated') =g= nm(jj,'cooled');
 376  eq_nmt(st)..                            nmt(st) =e= sum(i, nm(i,st));
 377  eq_nm(i,st)..                           x(i,st) =e= nm(i,st)/nmt(st);
 378   
 379  eq_Mw(ii)..                             Mw(ii) =e= sum(s, prop(s, 'p3') * y(ii,s));
 380  eq_Mwib(jj)..                           Mw(jj) =e= Mwib(jj);
 381  eq_mass(i,st)..                         mass(i,st) =e= nm(i,st) * Mw(i);
 382  eq_den(ii)..                            den(ii) =e= sum(s, prop(s,'p4')*y(ii,s));
 383  eq_vol(ii)..                            vol(ii) * den(ii) =e= mass(ii,'cooled');
 384  eq_n1(ii,k)..                           n(ii,k) =e= sum(s, v(s,k)*y(ii,s));
 385  eq_n2(jj,k)..                           n(jj,k) =e= vib(jj,k);
 386   
 387  *****Solubility calculations using UNIFAC
 388  eq_qc1(ii)..                            qc(ii) =e= sum(s,qs(s) * y(ii,s));
 389  eq_rc1(ii)..                            rc(ii) =e= sum(s,rs(s) * y(ii,s));
 390  eq_qc2(jj)..                            qc(jj) =e= qib(jj);
 391  eq_rc2(jj)..                            rc(jj) =e= rib(jj);
 392   
 393  *Combinatorial part of the activity coefficient
 394  eq_J(jj,st)..                           J(jj,st) * sum(i,x(i,st)*rc(i)) =e= rib(jj);
 395  eq_L(jj,st)..                           L(jj,st) * sum(i,x(i,st)*qc(i)) =e= qib(jj);
 396  eq_lngc(jj,st)..                        lngc(jj,st) =e= 1 - J(jj,st) + log(J(jj,st)) - 5*qib(jj)*(1 - J(jj,st)/L(jj,st) + log(J(jj,st)) - log(L(jj,st)));
 397   
 398  *Residual part of the activity coefficient
 399  eq_ps(k,m,st)..                         ps(k,m,st) =e= exp(-a(k,m)/T(st));
 400  eq_bib(jj,k,st)..                       bib(jj,k,st) =e= sum(m,eib(jj,m)*ps(m,k,st));
 401  eq_th(k,st)..                           th(k,st)*sum(i,x(i,st)*qc(i)) =e= sum(i,x(i,st)*GS(k,'Q')*n(i,k));
 402  eq_w(k,st)..                            w(k,st) =e= sum(m,th(m,st)*ps(m,k,st));
 403  eq_lngr(jj,st)..                        lngr(jj,st) =e= qib(jj)*(1-sum(k,th(k,st)*bib(jj,k,st)/w(k,st)-eib(jj,k)*(log(bib(jj,k,st)) - log(w(k,st)))));
 404   
 405  *Overall activity coefficient
 406  eq_lng(jj,st)..                         lng(jj,st) =e= lngc(jj,st) + lngr(jj,st);
 407   
 408  *Solubility oonstraint
 409  eq_solub1(st)..                         log(x('ibuprofen',st)) + lng('ibuprofen',st) =e= Hm('ibuprofen')/Rg * (1/Tm('ibuprofen') - 1/T(st));
 410   
 411  ***********************************Miscibility between s1 & s2**********
 412  eq_x12a(st)..                           x12('s1',st)*(x('s1',st)+x('s2',st))=e=x('s1',st);
 413  eq_x12b(st)..                           x12('s1',st)+x12('s2',st)=e=1;
 414   
 415  *Thresholding constraints
 416  eq_misc12(st)..                         misc12(st)=g=0.85;
