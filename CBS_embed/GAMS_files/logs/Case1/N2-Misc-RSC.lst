 112   
 113   
 114   
 115  parameter ps(k,m);
 116  ps(k,m)= exp(-a(k,m)/T);
 117  Display ps;
 118  parameter nib(i,k);
 119  nib('ibuprofen',k)= vib('ibuprofen',k);
 120  parameter qib;
 121  qib = sum(k,nib('ibuprofen',k)*Q(k));
 122  parameter rib;
 123  rib = sum(k,nib('ibuprofen',k)*R(k));
 124  parameter qs(s);
 125  qs(s) = sum(k,v(s,k)*Q(k));
 126  parameter rs(s);
 127  rs(s) = sum(k,v(s,k)*R(k));
 128   
 129  option decimals=4;
 130  Display qs,rs;
 131   
 132  parameter eib(i,k);
 133  eib('ibuprofen',k)=vib('ibuprofen',k)*Q(k)/qib;
 134  parameter bib(i,k);
 135  bib('ibuprofen',k)=sum(m,eib('ibuprofen',m)*ps(m,k));
 136  parameter yv(ii,s,c) 'store y values from previous iterations';
 137  parameter zv(c) 'store objective values from previous iterations';
 138  parameter xv(i,c) 'store mole fractions from previous iterations';
 139   
 140   
 141  POSITIVE VARIABLES
 142   
 143  x(i)             'liquid phase mole fraction of component i'
 144  rc(i)            'van der waals volume of component i'
 145  qc(i)            'van der waals area of component i'
 146  J(i),L(i)
 147   
 148  b(i,k)          'intermediates'
 149  th(k)
 150  w(k)
 151   
 152  xm(ii)
 153  th12(k)
 154  w12(k)
 155  sumr12
 156  sumq12
 157  b12(ii,k)
 158   
 159  FREE VARIABLES
 160  z            'objective function'
 161   
 162  lng(i)       'natural log of activity coefficient of ibuprofen'
 163  lngc(i)      'natural log of combinatorial activity coefficient of ibuprofen'
 164  lngr(i)      'natural log of residual activity coefficient of ibuprofen'
 165   
 166  d_lngc12(ii)  'differention of combinatorial act_coeff'
 167  d_lngr12(ii)  'differention of residual act_coeff'
 168  d_lng12(ii)   'differentiation of act_coef wrt to x(ii)'
 169  d_th12(k)     'diff of intermediate'
