 205   
 206   
 207  *********************choose only 2 solvents*************************************
 208  logic1(ii)..
 209  sum(s,y(ii,s))=e=1;
 210   
 211  **********************each solvent at most once*********************************
 212  logic2(s)..y('s1',s)+y('s2',s)=l=1;
 213   
 214  *****************************solvent ordering***********************************
 215  logic3(s,ss)$(ord(ss) <ord(s))..y('s1',s)+y('s2',s-ord(ss))=l=1;
 216   
 217   
 218   
 219  *******************************UNIFAC*******************************************
 220  eq_z..
 221  z=e=x('ibuprofen');
 222   
 223  eq_x..
 224  sum(i,x(i))=e=1;
 225   
 226  eq_n(ii,k)..
 227  n(ii,k)=e=sum(s,v(s,k)*y(ii,s));
 228   
 229   
 230  eq_qc(ii)..
 231  qc(ii)=e= sum(s,qs(s)*y(ii,s));
 232   
 233  eq_rc(ii)..
 234  rc(ii)=e= sum(s,rs(s)*y(ii,s));
 235   
 236  *combinatorial part of activity coefficient
 237  eq_J..J('ibuprofen')*sum(i,x(i)*rc(i))=e=rib;
 238  eq_L..L('ibuprofen')*sum(i,x(i)*qc(i))=e=qib;
 239   
 240  eq_lngc..
 241  lngc('ibuprofen')=e=1-J('ibuprofen')+log(J('ibuprofen'))-5*qib*(1-J('ibuprofen')/L('ibuprofen')+log(J('ibuprofen'))-log(L('ibuprofen')));
 242   
 243  *residual part of activity coefficient
 244  eq_th(k)..th(k)*sum(i,x(i)*qc(i))=e=sum(i,x(i)*Q(k)*n(i,k));
 245  eq_w(k)..w(k)=e=sum(m,th(m)*ps(m,k));
 246   
 247  eq_lngr..
 248  lngr('ibuprofen')=e=qib*(1-sum(k,th(k)*bib('ibuprofen',k)/w(k)-eib('ibuprofen',k)*(log(bib('ibuprofen',k))-log(w(k)))));
 249   
 250  *activity coefficient
 251  eq_lng..
 252  lng('ibuprofen') =e=  lngc('ibuprofen') + lngr('ibuprofen');
 253   
 254  *ibuprofen solubility constraint
 255  eq_solub..
 256  log(x('ibuprofen'))+ lng('ibuprofen') =e= Hm/Rg *(1/Tm-1/T);
 257   
 258  *****************Miscibility constraints between s1 & s2************************
 259  eq_xm1..xm('s1')*(x('s1')+x('s2'))=e=x('s1');
 260  eq_xm2..xm('s1')+xm('s2')=e=1;
 261   
 262  eq_sumr12..sumr12=e=xm('s1')*rc('s1')+(1-xm('s1'))*rc('s2');
 263  eq_sumq12..sumq12=e=xm('s1')*qc('s1')+(1-xm('s1'))*qc('s2');
 264   
 265  *differ of combinatorial activity coefficient, i.e. dlngc/dx(s1)
 266  eq_dlngc12..d_lngc12('s1')*power(sumr12,2)*sumq12=e=5*qc('s1')*(qc('s2')-qc('s1'))*power(sumr12,2)
 267                                          +(rc('s1')-rc('s2'))*(5*qc('s1')-1)*sumr12*sumq12
 268                                          +5*rc('s1')*(qc('s1')-qc('s2'))*sumr12*sumq12
 269                                          +((rc('s1')-rc('s2'))*(rc('s1')-5*rc('s1')*(sumq12)))*sumq12;
 270   
 271  eq_th12(k)..
 272  th12(k)*sumq12=e=xm('s1')*Q(k)*n('s1',k)+(1-xm('s1'))*Q(k)*n('s2',k);
 273   
 274  eq_w12(k)..w12(k)=e=sum(m,th12(m)*ps(m,k));
 275   
 276  eq_dth12(k)..
 277  d_th12(k)*power(sumq12,2)=e=((Q(k)*n('s1',k)- Q(k)*n('s2',k))*sumq12)-(xm('s1')*Q(k)*n('s1',k)+ xm('s2')*Q(k)*n('s2',k))*(qc('s1')-qc('s2'));
 278   
 279  eq_dw12(k)..d_w12(k)=e=sum(m,ps(m,k)*d_th12(m));
 280   
 281  eq_a12(k)..a12(k)*power(w12(k),2)=e=d_th12(k)*w12(k)-th12(k)*d_w12(k);
 282  eq_b12(ii,k)..b12(ii,k)=e=sum(m,Q(m)*n(ii,m)*ps(m,k)) ;
 283  eq_c12(k)..c12(k)*w12(k)=e=d_w12(k);
 284   
 285  *differ of residual activity coefficient, i.e. dlngr/dx(s1)
 286  *simplier form, not the general form of unifac
 287  eq_dlngr12..d_lngr12('s1')=e=-sum(k,a12(k)*b12('s1',k)+Q(k)*n('s1',k)*c12(k));
 288   
 289  *differ of activity coefficient,i.e. dlng/dx(s1)
 290  eq_dlng12..d_lng12('s1')=e= d_lngc12('s1')+d_lngr12('s1');
 291   
 292  eq_misc12..d_lng12('s1')*xm('s1')+1=g=0;
 293   
 294  *Integer cuts equation
 295  IntCut(c)$(dyn(c)).. sum((ii,s),yv(ii,s,c)*y(ii,s))-sum((ii,s),(1-yv(ii,s,c))*y(ii,s))=l=sum((ii,s),yv(ii,s,c))-1;
