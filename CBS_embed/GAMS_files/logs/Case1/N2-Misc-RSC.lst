 183  EQUATIONS
 184  eq_z                   'objective function maximise solubility of ibuprofen'
 185  eq_x                   'mole fraction constrain'
 186  eq_n(ii,k)
 187  eq_qc(ii)
 188  eq_rc(ii)
 189  eq_J,eq_L
 190  eq_lngc
 191  eq_th(k)
 192  eq_w(k)
 193  eq_lngr
 194  eq_lng
 195  eq_solub
 196  *equations for logical conditions
 197  logic1(ii),logic2(s)
 198  logic3(s,ss)
 199  *equations for miscibility constraints
 200  eq_xm1,eq_xm2
 201  eq_sumr12,eq_sumq12,eq_dlngc12,eq_th12(k),eq_dth12(k),eq_w12(k),eq_dw12(k),eq_a12(k),eq_b12(ii,k),eq_c12(k),eq_dlngr12,eq_dlng12,eq_misc12
 202  IntCut(c)
 203  ;
