 366  *********************INITIAL POINTS*********************************************
 367   
 368  *fixing of ibuprofen identity
 369  qc.fx('ibuprofen')=qib;
 370  rc.fx('ibuprofen')=rib;
 371  n.fx('ibuprofen',k)=vib('ibuprofen',k);
 372   
 373  *initilaization  of optimal solvent
 374  x.l('ibuprofen')=0.3;
 375  x.l('s2')=0.3;
 376  x.l('s1')=1-x.l('ibuprofen')-x.l('s2');
 377   
 378  *y.l('s1','chloroform')=1;
 379  *y.l('s2','water')=1;
 380   
 381  y.l('s1','ethylacetate')=1;
 382  y.l('s2','chloroform')=1;
 383   
 384  z.l=x.l('ibuprofen');
 385   
 386  n.l(ii,k)=sum(s,v(s,k)*y.l(ii,s));
 387   
 388  rc.l(ii)= sum(s,rs(s)*y.l(ii,s));
 389  qc.l(ii)= sum(s,qs(s)*y.l(ii,s));
 390   
 391  J.l('ibuprofen')=rib/sum(i,x.l(i)*rc.l(i));
 392  L.l('ibuprofen')=qib/sum(i,x.l(i)*rc.l(i));
 393   
 394  lngc.l('ibuprofen')=1-J.l('ibuprofen')+log(J.l('ibuprofen'))-5*qib*(1-J.l('ibuprofen')/L.l('ibuprofen')+log(J.l('ibuprofen'))-log(L.l('ibuprofen')));
 395   
 396  th.l(k)=sum(i,x.l(i)*Q(k)*n.l(i,k))/sum(i,x.l(i)*qc.l(i));
 397  w.l(k)=sum(m,th.l(m)*ps(m,k));
 398   
 399  lngr.l('ibuprofen')=qib*(1-sum(k,th.l(k)*bib('ibuprofen',k)/w.l(k)-eib('ibuprofen',k)*(log(bib('ibuprofen',k))-log(w.l(k)))));
 400   
 401  lng.l('ibuprofen') = lngc.l('ibuprofen') + lngr.l('ibuprofen');
 402   
 403  *initialisation of imiscibility calculations(s1,s2)
 404  xm.l('s1')=x.l('s1')/(x.l('s1')+x.l('s2'));
 405  xm.l('s2')=1-xm.l('s1');
 406   
 407  sumr12.l=xm.l('s1')*rc.l('s1')+(1-xm.l('s1'))*rc.l('s2');
 408  sumq12.l=xm.l('s1')*qc.l('s1')+(1-xm.l('s1'))*qc.l('s2');
 409   
 410  d_lngc12.l('s1')=5*qc.l('s1')*(qc.l('s2')-qc.l('s1'))/sumq12.l
 411                    +(rc.l('s1')-rc.l('s2'))*(5*qc.l('s1')-1)/sumr12.l
 412                    +5*rc.l('s1')*(qc.l('s1')-qc.l('s2'))/sumr12.l
 413                    +((rc.l('s1')-rc.l('s2'))*(rc.l('s1')-5*rc.l('s1')*(sumq12.l)))/power(sumr12.l,2);
 414   
 415  th12.l(k)=(xm.l('s1')*Q(k)*n.l('s1',k)+(1-xm.l('s1'))*Q(k)*n.l('s2',k))/sumq12.l;
 416   
 417  w12.l(k)=sum(m,th12.l(m)*ps(m,k));
 418   
 419  d_th12.l(k)=((Q(k)*n.l('s1',k)- Q(k)*n.l('s2',k))*sumq12.l-(xm.l('s1')*Q(k)*n.l('s1',k)+ xm.l('s2')*Q(k)*n.l('s2',k))*(qc.l('s1')-qc.l('s2')))/power(sumq12.l,2);
 420   
 421  d_w12.l(k)=sum(m,ps(m,k)*d_th12.l(m));
 422   
 423  a12.l(k)=(d_th12.l(k)*w12.l(k)-th12.l(k)*d_w12.l(k))/power(w12.l(k),2);
 424  b12.l(ii,k)=sum(m,Q(m)*n.l(ii,m)*ps(m,k));
 425  c12.l(k)=d_w12.l(k)/w12.l(k);
 426   
 427  d_lngr12.l('s1')=-sum(k,a12.l(k)*b12.l('s1',k)+Q(k)*n.l('s1',k)*c12.l(k));
 428   
 429  d_lng12.l('s1')=d_lngc12.l('s1')+d_lngr12.l('s1');
